{
  "rewrites": [
    { "source": "/levels", "destination": "/index.html" },
    { "source": "/learn", "destination": "/index.html" },
    { "source": "/level/:id", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}